          fill="url(#wing128)" opacity="0.8"/>
    
    <!-- Serpents -->
    <path d="M -8 -35 Q -15 -25 -12 -15 Q -15 -5 -8 5 Q -15 15 -12 25 Q -15 35 -8 45" 
          stroke="#4ade80" stroke-width="4" fill="none" opacity="0.8"/>
    <path d="M 8 -35 Q 15 -25 12 -15 Q 15 -5 8 5 Q 15 15 12 25 Q 15 35 8 45" 
          stroke="#f59e0b" stroke-width="4" fill="none" opacity="0.8"/>
    
